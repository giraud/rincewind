{
  "name": "rincewind-ocaml",
  "version": "0.1.0",
  "description": "OCaml workflow with Esy",
  "license": "MIT",
  "esy": {
    "build": "dune build -p #{self.name}",
    "release": {
      "includePackages": [
        "root",
        "@opam/camomile"
      ],
      "releasedBinaries": ["FooApp.exe"]
    }
  },
  "buildDirs": {
    "test": {
      "require": ["rincewind/lib", "rely.lib"],
      "bin": {
        "TestRincewind.exe": "TestRincewind.re"
      }
    },
    "library": {},
    "executable": {
      "require": ["rincewind/lib", "@opam/camomile"],
      "bin": {
        "rincewind.exe": "rincewind.ml"
      }
    }
  },
  "scripts": {
    "package": "esy build&& esy cp \"#{self.target_dir}/default/bin/rincewind.exe\" distrib/rincewind.exe",
    "test": "esy b dune runtest"
  },
  "dependencies": {
    "@opam/dune": "*",
    "ocaml": "~4.06.1",
    "pesy": "*",
    "@reason-native/rely": "*"
  },
  "devDependencies": {
    "ocaml": "~4.06.1"
  },
  "resolutions": {
    "pesy": "esy/pesy#ba6359f25621280a8105d2ffc99d75d849c0d95a"
  }
}