{
  "name": "rincewind",
  "version": "0.1.0",
  "description": "Cmt annotation extractor",
  "license": "MIT",
  "esy": {
    "build": "dune build -p #{self.name}",
    "release": {
      "includePackages": [
        "root"
      ],
      "releasedBinaries": [
        "rincewind.exe"
      ]
    }
  },
  "buildDirs": {
    "test": {
      "require": [
        "rincewind/lib",
        "rely.lib"
      ],
      "bin": {
        "TestRincewind.exe": "TestRincewind.re"
      }
    },
    "library": {},
    "executable": {
      "require": [
        "rincewind/lib"
      ],
      "bin": {
        "rincewind.exe": "rincewind.ml"
      }
    }
  },
  "scripts": {
    "package": "esy build&& esy cp \"#{self.target_dir}/default/executable/rincewind.exe\" _release/rincewind.exe",
    "test": "esy b dune runtest"
  },
  "dependencies": {
    "@opam/dune": "*",
    "ocaml": "~4.06.1",
    "pesy": "*"
  },
  "devDependencies": {
    "ocaml": "~4.06.1",
    "bintray": "*",
    "@reason-native/rely": "*"
  }
}
